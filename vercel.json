{
  "version": 2,
  "builds": [
    {
      "src": "legal-backend2/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend1/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/docs/(.*)",
      "destination": "/legal-backend2/src/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend1/build/index.html"
    }
  ]
} 